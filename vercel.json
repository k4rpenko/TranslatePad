{
  "version": 2,
  "builds": [
    {
      "src": "./Server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "./website/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Server/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "website/$1",
      "headers": { "Cache-Control": "s-maxage=1, stale-while-revalidate" }
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}