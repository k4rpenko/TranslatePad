{
  "version": 2,
  "builds": [
    {
      "src": "./Server_C/",
      "use": "@now/dotnet"
    },
    {
      "src": "./website/package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Server_C/"
    },
    {
      "source": "/(.*)",
      "destination": "/website/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
